  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "clean": "rm -rf .next",
    "generate:w_auth": "prisma generate --schema=prisma.w_auth/schema.prisma",
    "generate:atsuocoder": "prisma generate --schema=prisma.atsuocoder/schema.prisma",
