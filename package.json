    "@toolpad/core": "^0.16.0",
    "brace": "^0.11.1",
    "highlight.js": "^11.11.1",
    "material-symbols": "^0.33.0",
    "next": "15.4.4",
    "react": "19.1.0",
    "react-ace": "^14.0.1",
    "react-dom": "19.1.0",
    "react-markdown": "^10.1.0",
    "react-syntax-highlighter": "^15.6.1",
