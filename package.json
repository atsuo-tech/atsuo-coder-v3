    "dbpush:w_auth": "prisma db push --schema=prisma.w_auth/schema.prisma",
    "dbpush:atsuocoder": "prisma db push --schema=prisma.atsuocoder/schema.prisma",
    "dbpush": "npm run db push:w_auth && npm run db push:atsuocoder"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
