    "@svgr/webpack": "^8.1.0",
    "material-symbols": "^0.33.0",
    "next": "15.4.4",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "prisma": "^6.12.0",
    "typescript": "^5"
  }
}